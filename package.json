{
    "version": "0.1.3",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/ligaopeng123/rtc-cli.git"
    },
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "bin": {
        "rtc": "bin/index.js"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "push": "npm publish --access public",
        "link": "npm uninstall -g rtc && npm link --force",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "@gaopeng123/rtc-cli",
    "author": "ligaopeng123",
    "module": "dist/rtc-cli.esm.js",
    "size-limit": [
        {
            "path": "dist/rtc-cli.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/rtc-cli.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.2",
        "@types/lodash.template": "^4.5.0",
        "@types/node": "^16.4.10",
        "@types/prompts": "^2.0.14",
        "husky": "^7.0.1",
        "size-limit": "^5.0.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "cac": "^6.7.3",
        "chalk": "^4.1.2",
        "download-git-repo": "^3.0.2",
        "fast-glob": "^3.2.7",
        "lodash.template": "^4.5.0",
        "ora": "^5.4.1",
        "prompts": "^2.4.1"
    }
}
